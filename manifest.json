{
  "manifest_version": 3,
  "name": "Voispeed Link Converter",
  "version": "1.3",
  "description": "Converte link tel: con numeri telefonici in URI VOIspeed.",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Voispeed Link Converter"
  },
  "content_scripts": [
    {
      "matches": ["https://s1.term/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
